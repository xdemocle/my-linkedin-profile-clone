{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/assets/*",
    "/_astro/*",
    "/favicon.ico",
    "/manifest.json",
    "/sw.js"
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/en/",
      "status": 302
    },
    {
      "src": "^/en$",
      "dest": "/en/",
      "status": 301
    },
    {
      "src": "^/it$",
      "dest": "/it/",
      "status": 301
    },
    {
      "src": "^/fr$",
      "dest": "/fr/",
      "status": 301
    },
    {
      "src": "^/es$",
      "dest": "/es/",
      "status": 301
    },
    {
      "src": "^/ar$",
      "dest": "/ar/",
      "status": 301
    },
    {
      "src": "^/en/.*",
      "dest": "/en/index.html"
    },
    {
      "src": "^/it/.*",
      "dest": "/it/index.html"
    },
    {
      "src": "^/fr/.*",
      "dest": "/fr/index.html"
    },
    {
      "src": "^/es/.*",
      "dest": "/es/index.html"
    },
    {
      "src": "^/ar/.*",
      "dest": "/ar/index.html"
    }
  ]
}
