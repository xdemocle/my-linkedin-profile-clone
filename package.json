{
  "name": "my-linkedin-profile-clone",
  "description": "This is a LinkedIn profile clone built with React, TypeScript, and Vite. The project is configured for deployment on Cloudflare Pages and uses Bun as the package manager.",
  "version": "0.0.1",
  "author": "xdemocle",
  "license": "MIT",
  "homepage": "https://github.com/xdemocle/my-linkedin-profile-clone",
  "repository": {
    "type": "git",
    "url": "https://github.com/xdemocle/my-linkedin-profile-clone.git"
  },
  "type": "module",
  "engines": {
    "node": ">=22.19.0"
  },
  "packageManager": "bun@1.2.15",
  "bun": {
    "env": {
      "NODE_ENV": "development"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "bun run build && wrangler pages dev",
    "deploy": "bun run build && wrangler pages deploy",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/bun": "^1.2.23",
    "@types/node": "^24.5.2",
    "@types/react-dom": "^19.1.9",
    "@types/react": "^19.1.15",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "http-server": "^14.1.1",
    "postcss-cli": "^11.0.1",
    "postcss": "^8.5.6",
    "react-dom": "^19.1.1",
    "react": "^19.1.1",
    "tailwindcss": "^4.1.13",
    "typescript-eslint": "^8.44.1",
    "typescript": "~5.9.2",
    "vite-prerender-plugin": "^0.5.12",
    "vite": "^7.1.7",
    "wrangler": "^4.40.2"
  }
}